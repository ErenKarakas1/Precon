{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "Precon",
  "mainBinaryName": "Precon",
  "version": "0.1.0",
  "identifier": "com.precon.precon",
  "plugins": {},
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Precon",
        "width": 1920,
        "height": 1080
      }
    ],
    "security": {
      "csp": null,
      "capabilities": ["main-capabilities"]
    }
  }
}
